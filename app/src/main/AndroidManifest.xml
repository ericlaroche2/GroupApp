<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ericpc.groupapp">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.ericpc.groupapp.AutoActivity"
            android:label="@string/title_activity_auto"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name="com.example.ericpc.groupapp.DisplayList"
            android:label="@string/title_activity_display"/>

        <activity android:name="com.example.ericpc.groupapp.AutoDetailsActivity"/>

        <activity android:name="com.example.ericpc.groupapp.AutoMain"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name="com.example.ericpc.groupapp.AutoStatistics"
            android:label="@string/title_activity_statistics"></activity>
    </application>

</manifest>